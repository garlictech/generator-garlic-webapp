{
  "name": "@<%= conf.scope %>/<%= conf.appNameKC %>",
  "version": "0.0.0",
  "description": "<%= conf.appNameAsIs %>",
  "main": "dist/ngx/main.bundle.js",
  "typings": "dist/ngx/lib/ngx/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "license": "SEE LICENSE IN license.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= conf.scope %>/<%= conf.appNameKC %>.git"
  },
  "author": {
    "name": "<%= conf.scopeCC %>",
    "email": "contact@<%= conf.scope %>.com",
    "url": "http://www.<%= conf.scope %>.com"
  },
  "contributors": [
    "Zsolt R. Molnar <zsolt@zsoltmolnar.hu> (http://www.zsoltmolnar.hu)"
  ],
  "keywords": [
    "<%= conf.appName %>",
    "<%= conf.appNameKC %>",
    "<%= conf.scope %>"
  ],
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=5.0.0"
  },
  "scripts": {
    "setup": "docker/setup.sh",
    "ngx:build:dev": "docker/build-dev.sh $@",
    "ngx:build:dev:all": "docker/build-dev.sh --pull --no-cache",
    "ngx:build:prod": "docker/build-prod.sh",
    "ngx:start:dev": "docker/start-dev.sh",
    "ngx:stop:dev": "docker/stop-devsh",
    "ngx:unittest": "docker/unittest.sh unittest",
    "ngx:unittest:watch": "docker/unittest.sh unittest:watch",
    "backend:build": "make build",
    "backend:build:all": "make build-all",
    "backend:start:dev": "make start",
    "backend:stop:dev": "make stop",
    "backend:unittest": "make unittest",
    "lint": "docker/unittest.sh lint",
    "ngx:bash": "docker/bash-ngx.sh",
    "backend:bash": "docker/bash-backend.sh",
    "clean": "docker/npm.sh clean",
    "commit": "docker/commit.sh",
    "release": "docker/semantic-release.sh",
    "start": "docker/start-dev.sh",
    "npm": "docker/npm.sh",
    "travis": "docker/travis.sh $@"
  },
  "dependencies": {},
  "devDependencies": {},
  "config": {
    "dockerWorkflowVersion": "<%= conf.dockerWorkflowVersion %>"
  }
}