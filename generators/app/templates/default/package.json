{
  "name": "@garlictech/<%= appName %>",
  "version": "0.0.1",
  "description": "<%= appNameAsIs %>",
  "main": "src/index.coffee",
  "license": "SEE LICENSE IN license.txt",
  "repository": {
    "type": "git",
    "url": "https://molnarzs@bitbucket.org/gtrack/<%= appName %>.git"
  },
  "author": "zsolt@zsoltmolnar.hu",
  "keywords": [
    "<%= appName %>"
  ],
  "bugs": {
    "url": "https://bitbucket.org/gtrack/<%= appName %>/issues?status=new&status=open"
  },
  "homepage": "https://bitbucket.org/gtrack/<%= appName %>/wiki/Home",
  "devDependencies": {
    "@garlictech/workflows-client": "*",
    "angular-mocks": "1.5.x",
    "coffee-script": "1.x.x",
    "gulp": "^3.9.1"
  },
  "dependencies": {
    "angular": "1.5.x",
    "lodash": "^4.x.x"
  },
  "files": ["dist"],
  "engines": {
    "npm" : ">=2.7.0"
  },
  "scripts": {
    "start": "node node_modules/.bin/webpack-dev-server --inline --hot --watch",
    "unittest": "KARMA_TEST=unit node node_modules/karma/bin/karma start karma.conf.coffee",
    "systemtest": "KARMA_TEST=system node node_modules/karma/bin/karma start karma.conf.coffee",
    "build": "node node_modules/webpack/bin/webpack.js",
    "protractor-start": "node_modules/protractor/bin/webdriver-manager start",
    "protractor-test": "node_modules/protractor/bin/protractor ./src/test/protractor/config.coffee",
    "protractor-setup": "node_modules/protractor/bin/webdriver-manager update --standalone",
        "start-watch": "webpack --watch --cache false",
    "start-dev": "ionic emulate -l -c --address localhost ios",
    "webpack-profile": "webpack --profile --json > webpack-stats.json"
  }
}
