{
  "name": "@<%= conf.scope %>/<%= conf.appNameKC %>",
  "version": "0.0.1",
  "description": "<%= conf.appNameAsIs %>",
  "main": "dist/server.js",
  "license": "SEE LICENSE IN license.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= conf.scope %>/<%= conf.appNameKC %>.git"
  },
  "author": {
    "name": "<%= conf.scopeCC %>",
    "email": "contact@<%= conf.scope %>.com",
    "url": "http://www.<%= conf.scope %>.com"
  },
  "contributors": [
    "Zsolt R. Molnar <zsolt@zsoltmolnar.hu> (http://www.zsoltmolnar.hu)"
  ],
  "keywords": [
    "<%= conf.appName %>",
    "<%= conf.appNameKC %>",
    "<%= conf.scope %>"
  ],
  "bugs": {
    "url": "https://github.com/<%= conf.scope %>/<%= conf.appNameKC %>/issues"
  },
  "homepage": "https://github.com/<%= conf.scope %>/<%= conf.appNameKC %>/wiki/Home",
